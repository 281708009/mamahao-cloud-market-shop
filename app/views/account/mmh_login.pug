extends ../layouts/initial.pug
block title
    title 立即登录
block meta
    meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0')
block css
    style.
        html,body{ background: #F6F6F6;}
        .mamahao-login{ padding: 0 15px;}
        .mamahao-login .logo{ padding: 30px 0; text-align: center;}
        .mamahao-login .logo span{ display: block; width: 80px; height: 80px; margin: 0 auto; border-radius: 10px; background: url('data:image/png;base64,') no-repeat #fff; background-size: cover;}
        .mamahao-login .logo em{ font-size: 17px; color: #111; padding-top: 10px; line-height: 1; display: block;}
        .mamahao-login dl.tips{ padding: 30px 0; border-top: 1px solid #dcdcdc;}
        .mamahao-login dl.tips dt{ color: #111;}
        .mamahao-login dl.tips dd{ font-size: 13px;}
        .mamahao-login dl.tips dd p{ text-indent: 1em; position: relative; padding: 5px 0;}
        .mamahao-login dl.tips dd p:before{ width: 3px; height: 3px; border-radius: 50%; background: #666; left: 3px; top: 15px; content: ""; position: absolute;}
block js
    script seajs.run("mamahao/login");
block content
    .mamahao-login
        .logo
            span
            em 妈妈好APP
        dl.tips
            dt 您当前未登录
            dd
                p 需要登录才能使用妈妈好相关业务功能
        .button
            button.u-btn.success.alone.max(onclick='APP.open("login");') 立即登录