include ../../includes/mixin.pug
-var inlet = 1, extra = '&shopId=' + request.shopId;
.goods
    .tab.js-goods-tab
        ul
            each t, i in tab
                li(class=(i == 0 && "active"), data-id=t.id)
                    a(href='javascript:;') #{t.name}
    ul.u-goods-list.pagination.clearfix
        +goodsList(goodsList)