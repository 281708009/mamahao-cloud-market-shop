extends ../layouts/initial.pug
block title
    title 支付成功
block meta
    script(type='text/javascript').
        var sw = window.screen.width > 414 ? 414 : window.screen.width;
        var phoneScale = parseInt(sw) / 750;
        document.write('<meta name="viewport" content="width=750, minimum-scale=' + phoneScale + ', maximum-scale=' + phoneScale + ', target-densitydpi=device-dpi">');
block css
    link(rel='stylesheet', href='http://s.mamhao.cn/m/v1/style/pay-code.css')
    style.
        body {font-size: 30px; background: #FFF;}
block js
    script seajs.use('app/pay_code_success');
block content
    // 领取成功
    section.m-code-success
        .header
            img(src='data:image/png;base64,', alt='')
        dl.tip
            dt 付款成功啦！
            dd 妈妈好送您180元红包，输入手机号领取吧！
        .form
            .content
                .info
                    strong 180
                    span 元
                    p 红包
                ul
                    li
                        input.js-mobile(type='tel', placeholder='请输入手机号码', maxlength='11')
                    li
                        input.js-code(type='tel', placeholder='请输入验证码', maxlength='6')
                        button.js-get-code 获取验证码
                .submit
                    button.js-submit 立即领取
    // 弹出层 .show
    .m-pop-all
        .m-pop
            .title
                .close
            .content-border
                .content
                    p 您已成功领取新人红包！
                    .volume
                        div
                            p
                                strong 180
                                span 元
                    a.download(href='http://app.mamahao.com/') 下载“妈妈好”即可使用
    // 下载妈妈好
    section.MA-download-pop-all.show
        .space
        .MA-download-pop
            a(href='http://app.mamahao.com/')
                .logo
                dl
                    dt 妈妈好
                    dd
                        p 好孩子集团用户服务平台
                        p.star
                em 下载
            del.js-close-app 关闭
    // JS脚本;
    script.
        var VM = {
            openId: "#{openId}",
            isWeChat: #{isWeChat},
            isAlipay: #{isAlipay},
            isMamahao: #{isMamahao}
        };